@startuml

entity "ALUMNOS" {
  +ID_ALUMNO: INT
  +NOMBRE: VARCHAR
  +APELLIDO1: VARCHAR
  +APELLIDO2: VARCHAR
  +DNI: VARCHAR
}

entity "CURSOS" {
  +ID_CURSO: INT
  +NOMBRE_CURSO: VARCHAR
  +TIPO_CURSO: ENUM('Informática', 'Inglés')
}

entity "MATERIAS" {
  +ID_MATERIA: INT
  +NOMBRE_MATERIA: VARCHAR
  +ID_CURSO: INT
}

entity "PROFESORES" {
  +ID_PROFESOR: INT
  +NOMBRE: VARCHAR
  +APELLIDO1: VARCHAR
  +APELLIDO2: VARCHAR
  +DNI: VARCHAR
}

entity "MATRICULAS" {
  +ID_ALUMNO: INT
  +ID_CURSO: INT
  +FECHA_MATRICULA: DATE
}

entity "PROFESORES_MATERIAS" {
  +ID_PROFESOR: INT
  +ID_MATERIA: INT
}

ALUMNOS --o MATRICULAS : "Matriculado en"
CURSOS --o MATRICULAS : "Tiene"
CURSOS --o MATERIAS : "Contiene"
MATERIAS --o PROFESORES_MATERIAS : "Es impartida por"
PROFESORES --o PROFESORES_MATERIAS : "Imparte"

@enduml
