//como funciona for
var AccionPrepararPantalla = function() {
  FuncionCargarEventosIniciales();
  FuncionCargarDatosIniciales();
}

var AccionAcpetar=function();
  let Valor=1;
  for (let i=5;i>0;i--) {
    Valor = Valor * i;
  }

FuncionMostrarResultado(Valor,true);

