CREATE TABLE `northwind`.`stores` (
    `StoreID` INT AUTO_INCREMENT NOT NULL,
    `StoreName` VARCHAR(100) NOT NULL,
    `ContactName` VARCHAR(100),
    `Address` VARCHAR(255),
    `City` VARCHAR(100),
    `PostalCode` VARCHAR(20),
    `Country` VARCHAR(50),
    `Phone` VARCHAR(50),
    PRIMARY KEY (`StoreID`)
);
---
